<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="select_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="take_photo_anim"
        parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/pop_exit_anim</item>
    </style>

    <style name="title_18_withe_style">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="white_14_style">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="title_16_withe_style">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/white</item>
    </style>
    <style name="title_16_black_style">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="title_30_withe_style">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">30sp</item>
    </style>
</resources>